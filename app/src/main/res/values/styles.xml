<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="SeekBarCustomStyle" parent="Widget.AppCompat.SeekBar">
        <item name="android:thumbTint">@color/someElementColor</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <item name="android:background">@color/light_background</item>
    </style>

    <style name="AppTheme.Dark" parent="Theme.AppCompat">
        <item name="android:background">@color/dark_background</item>
        <item name="android:buttonStyle">@style/ButtonStyle.Dark</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:tint">@android:color/white</item>
    </style>


    <style name="ButtonStyle.Dark" parent="Widget.AppCompat.Button">
        <item name="android:backgroundTint">@color/buttons_dark</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:drawableTint">@android:color/white</item>
    </style>

</resources>